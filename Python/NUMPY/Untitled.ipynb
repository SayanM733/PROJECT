   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "var :  [ 1 40  3  4]\n",
      "view :  [ 1 40  3  4]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "x = np.array([1,2,3,4])\n",
    "\n",
    "vi = x.view()\n",
    "x[1]=40\n",
    "print(\"var : \",x)\n",
    "print(\"view : \",vi)\n"
   ]
