    "# COPY & VIEW"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "5c778eaf",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "var :  [1 2 3 4]\n",
      "copy :  [1 2 3 4]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "var = np.array([1,2,3,4])\n",
    "\n",
    "co = var.copy()\n",
    "\n",
    "print(\"var : \",var)\n",
    "print(\"copy : \",co)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
